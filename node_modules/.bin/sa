#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luoqi/git/github/soybean-admin/packages/scripts/node_modules:/home/luoqi/git/github/soybean-admin/packages/node_modules:/home/luoqi/git/github/soybean-admin/node_modules:/home/luoqi/git/github/node_modules:/home/luoqi/git/node_modules:/home/luoqi/node_modules:/home/node_modules:/node_modules:/home/luoqi/git/github/soybean-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luoqi/git/github/soybean-admin/packages/scripts/node_modules:/home/luoqi/git/github/soybean-admin/packages/node_modules:/home/luoqi/git/github/soybean-admin/node_modules:/home/luoqi/git/github/node_modules:/home/luoqi/git/node_modules:/home/luoqi/node_modules:/home/node_modules:/node_modules:/home/luoqi/git/github/soybean-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/tsx" ]; then
  exec "$basedir/tsx"  "$basedir/../@sa/scripts/bin.ts" "$@"
else
  exec tsx  "$basedir/../@sa/scripts/bin.ts" "$@"
fi
